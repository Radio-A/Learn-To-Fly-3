	<script type="text/javascript">
		$(document).ready(function(){

		$('.widget-sign-in input')
		    .each(function(){
			   var input = $(this);
			   $('label[for="' + $(this).attr('id') + '"]').click(function() {
					input.focus();
				});
				if ($(this).val()!= ''){
					$('label[for="' + $(this).attr('id') + '"]').hide();
				}
			}).focus(function() {
					$('label[for="' + $(this)[0].id + '"]').hide();
			}).blur(function() {
				if($(this).val() == '') {
				$('label[for="' + $(this)[0].id + '"]').show();
				}
			}).change(function(){
				if($(this).val() != '') {
				$('label[for="' + $(this)[0].id + '"]').hide();
				}
			});

		});
	</script>

