<script>
$(window).scroll(function () {
    var $heightScrolled = $(window).scrollTop();
    var $defaultHeight = 100;

    if ($heightScrolled < $defaultHeight) {
        $('.freeweb').removeClass("b")
    }
    else {
        $('.freeweb').addClass("b")
    }

});  
</script>

